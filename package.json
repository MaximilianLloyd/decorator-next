{
  "name": "decorator-next",
  "version": "0.0.1",
  "description": "",
  "scripts": {
    "prettier": "prettier",
    "lint": "eslint",
    "tailwind": "npx tailwindcss -i ./client/main.css -o ./public/dist.css --watch",
    "cypress:open": "cypress open",
    "cypress:open:reference": "IS_OLD=true cypress open",
    "cypress:run": "cypress run",
    "cypress:run:reference": "IS_OLD=true cypress run",
    "dev": "concurrently \"vite\" \"npm run dev-server\" \"npm run tailwind\"",
    "dev-server": "nodemon --ext ts --watch views/ --watch server.ts --exec ts-node --project tsconfig.node.json -r tsconfig-paths/register server.ts",
    "build": "vite build --debug",
    "serve": " NODE_ENV=production ts-node --project tsconfig.node.json -r tsconfig-paths/register server",
    "prepare": "husky install"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@fullhuman/postcss-purgecss": "^5.0.0",
    "@navikt/ds-tokens": "^4.7.2",
    "@testing-library/cypress": "^9.0.0",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/mustache-express": "^1.2.2",
    "@types/node": "^20.4.2",
    "@types/ws": "^8.5.5",
    "autoprefixer": "^10.4.14",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "lint-staged": "^13.2.3",
    "postcss": "^8.4.27",
    "postcss-prefix-selector": "^1.16.0",
    "prettier": "^3.0.0",
    "vite": "^4.4.7",
    "ws": "^8.13.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.13",
    "@typescript-eslint/eslint-plugin": "latest",
    "@typescript-eslint/parser": "latest",
    "common-tags": "^1.8.2",
    "concurrently": "^8.2.0",
    "cypress": "^12.17.2",
    "eslint": "latest",
    "husky": "^8.0.0",
    "nodemon": "^3.0.1",
    "rollup-plugin-minify-html-literals-v3": "^1.3.3",
    "tailwindcss": "^3.3.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.6",
    "vite-tsconfig-paths": "^4.2.0",
    "zod": "^3.21.4"
  },
  "packageManager": "yarn@3.6.1",
  "lint-staged": {
    "*.{js,ts,tsx}": "eslint --cache --fix",
    "*.{js,ts,tsx,css,md}": "prettier --write"
  }
}
